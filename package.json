{
  "name": "fast-json-stringify",
  "version": "5.4.0",
  "description": "Stringify your JSON at max speed",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "bench": "node ./benchmark/bench.js",
    "bench:cmp": "node ./benchmark/bench-cmp-branch.js",
    "bench:cmp:ci": "node ./benchmark/bench-cmp-branch.js --ci",
    "benchmark": "node ./benchmark/bench-cmp-lib.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test:typescript": "tsc --project ./test/types/tsconfig.json && tsd",
    "test:unit": "tap -J test/*.test.js test/**/*.test.js",
    "test": "npm run test:unit && npm run test:typescript"
  },
  "precommit": ["lint", "test"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fast-json-stringify.git"
  },
  "keywords": [
    "json",
    "stringify",
    "schema",
    "fast"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fast-json-stringify/issues"
  },
  "homepage": "https://github.com/fastify/fast-json-stringify#readme",
  "devDependencies": {
    "@fastify/pre-commit": "^2.0.2",
    "@sinclair/typebox": "^0.25.2",
    "benchmark": "^2.1.4",
    "cli-select": "^1.1.2",
    "compile-json-stringify": "^0.1.2",
    "is-my-json-valid": "^2.20.0",
    "luxon": "^3.0.1",
    "proxyquire": "^2.1.3",
    "semver": "^7.1.0",
    "simple-git": "^3.7.1",
    "standard": "^17.0.0",
    "tap": "^16.0.1",
    "tsd": "^0.24.1",
    "typescript": "^4.0.2",
    "webpack": "^5.40.0"
  },
  "dependencies": {
    "@fastify/deepmerge": "^1.0.0",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-uri": "^2.1.0",
    "rfdc": "^1.2.0"
  },
  "standard": {
    "ignore": [
      "schema-validator.js"
    ]
  },
  "runkitExampleFilename": "./examples/example.js",
  "tsd": {
    "directory": "test/types"
  }
}
